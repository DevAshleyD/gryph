{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-mv-js && npm run build-mv-js-map",
    "build-mv-js": "mv build/static/js/*.js ../public/js/bundle.js",
    "build-mv-js-map": "mv build/static/js/*.js.map ../public/js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
